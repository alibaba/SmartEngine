           name="Test Smart Process">

	<process>
		<activity id="startOrder" />
		<sequenceFlow id="doStart" sourceRef="startOrder" targetRef="payment"/>

		<activity id="payment" />
		<sequenceFlow id="doPayment" sourceRef="payment" targetRef="shipment"/>
